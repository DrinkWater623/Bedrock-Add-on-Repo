{
	"$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.2.json",
	"author": "DrinkWater623/PinkSalt623 aka Nikki DW",
	"name": "Wither_Alert",
	"packs": {
		"behaviorPack": "./packs/BP",
		"resourcePack": "./packs/RP"
	},
	"regolith": {
		"dataPath": "./packs/data",
		"filterDefinitions": {
			"jsonte": {"url":"github.com/MCDevKit/regolith-library","version":"2.12.0"},
			"dot_ignore": {"runWith":"python","script":"./filters/dot_ignore.py"},
			"empty_ignore": {"runWith":"python","script":"./filters/empty_ignore.py"},
			"mani_fest": {"runWith":"nodejs","script":"./filters/mani_fest.js"}
		},
		"profiles": {
			"build": {
				"export": {"readOnly":false,"target":"local","bpName": "'BP'","rpName": "'void'"},				
				"filters": [							
					/*{"filter":"dot_ignore"},
					{"filter":"jsonte"},
					{"filter":"empty_ignore"},*/
					{
						"filter": "mani_fest",
						"settings": {	
							"Debug":false,						
							"name": "Stable 1.11.0: Wither Alert",
							"description": "Gives Locations of Withers",
							"min_engine_version": [1,21,0],
							//because I am using the dynamicProperties - need static header UUID
							"BP":{
								"header_uuid": "ea61f455-31fd-4659-8607-5e7f3788c144",
								"module_uuid": "ef06f6ca-d207-4b6b-8ffb-3456a4ed3032",
								"dependencies":{"module_name":"@minecraft/server","version":"stable"},
								"script_eval":true
							}					
						}
					}
				]
			},
			"betaBuild": {
				"export": {"readOnly":false,"target":"local","bpName": "'BP'","rpName": "'void'"},				
				"filters": [							
					/*{"filter":"dot_ignore"},
					{"filter":"jsonte"},
					{"filter":"empty_ignore"},*/
					{
						"filter": "mani_fest",
						"settings": {	
							"Debug":false,						
							"name": "Beta 1.12.0: Wither Alert",
							"description": "Gives Locations of Withers",
							"min_engine_version": [1,21,0],
							"BP":{
								"header_uuid": "a2098444-c84b-41bd-878e-cf1052601a35",
								"module_uuid": "788c7736-9aa8-4308-b9b5-47a2ece2eb91",
								"dependencies":{"module_name":"@minecraft/server","version":"1.12.0-beta"},
								"script_eval":true
							}					
						}
					}
				]
			},
			"stable": {
				"export": {"readOnly":false,"target":"development","bpName":"project.name+'_'+profile+'_bp'","rpName": "'void'"},
				"filters": [
					/*{"filter":"dot_ignore"},
					{"filter":"jsonte"},
					{"filter":"empty_ignore"},*/			
					{
						"filter": "mani_fest",
						"settings": {
							"Debug":false,
							"name": "Dev: Stable 1.11.0: Wither Alert",
							"description": "Gives Locations of Withers",
							"min_engine_version": [1,21,0],
							"BP":{
								"header_uuid": "ea61f455-31fd-4659-8607-5e7f3788c144",
								"module_uuid": "ef06f6ca-d207-4b6b-8ffb-3456a4ed3032",
								"dependencies":{"module_name":"@minecraft/server","version":"stable"},
								"script_eval":true
							}
							/*,
							"RP": {
								"header_uuid":"5073d92a-fd38-4dc6-a462-5f689dac7742",
								"module_uuid":"286222b2-87b0-4678-89f8-67ff07b40e69"
							}*/							
						}
					}
				]
			},
			"beta": {
				"export": {"readOnly":false,"target":"development","bpName":"project.name+'_'+profile+'_bp'","rpName": "'void'"},
				"filters": [
					/*{"filter":"dot_ignore"},
					{"filter":"jsonte"},
					{"filter":"empty_ignore"},*/			
					{
						"filter": "mani_fest",
						"settings": {
							"Debug":false,
							"name": "Dev: Beta 1.12.0: Wither Alert",
							"description": "Requires Beta API. Gives Locations of Withers",
							"min_engine_version": [1,21,0],														
							"BP":{
								"header_uuid": "a2098444-c84b-41bd-878e-cf1052601a35",
								"module_uuid": "788c7736-9aa8-4308-b9b5-47a2ece2eb91",
								"dependencies":{"module_name":"@minecraft/server","version":"1.12.0-beta"},
								"script_eval":true
							}														
						}
					}
				]
			},
			"preview": {
				"export": {"readOnly":false,"target":"preview","bpName":"project.name+'_'+profile+'_bp'","rpName": "'void'"},
				"filters": [
					/*{"filter":"dot_ignore"},
					{"filter":"jsonte"},
					{"filter":"empty_ignore"},*/
					{
						"filter": "mani_fest",
						"settings": {
							"Debug":false,
							"name": "Dev: Stable 1.12.0: Wither Alert",
							"description": "Gives Locations of Withers",
							"min_engine_version": [1,21,20],														
							"BP":{
								"header_uuid": "a2098444-c84b-41bd-878e-cf1052601a35",
								"module_uuid": "788c7736-9aa8-4308-b9b5-47a2ece2eb91",
								"dependencies":{"module_name":"@minecraft/server","version":"1.14.0-beta"},
								"script_eval":true
							}														
						}
					}
				]
			}
		}
	}
}