              {"{{#0..4}}":{"max_uses":5,"reward_exp":true,"wants":[{"item":"minecraft:emerald","quantity":1}],"gives":[{"type":"item","item":"minecraft:skull","quantity":1,"functions":[{"function":"set_data","data":"{{=value}}"}]}]}}              
            ]
