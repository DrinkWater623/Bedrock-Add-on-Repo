            "num_to_select": 1,
            "trades": [
              {
                "{{#armor_list}}":  {
                  "{{#armor_material_list.filter(x => x.level >= 4)}}":  {
                  "max_uses":5,"reward_exp":true,
                  "wants": [{"item":"{{=enchanted_armor_currency}}","quantity":"{{=enchanted_armor_cost}}","price_multiplier":0.0},{"item":"dw623:trading_coin","quantity":1,"price_multiplier":0.00}],
                  "gives": [{"item":"minecraft:{{ore_name}}_{{armor_name}}","quantity":1,
                  "functions": [{"function":"enchant_with_levels","treasure":false,"levels":{"min":39,"max":39}}]}]                   
                  }
                }
              },
              {
                "{{#armor_list}}":  {
                  "{{#armor_material_list.filter(x => x.level == 5)}}":  {
                  "max_uses":5,"reward_exp":true,
                  "wants": [{"item":"{{armor_currency}}","quantity":"{{=armor_cost}}","price_multiplier":0.00}],
