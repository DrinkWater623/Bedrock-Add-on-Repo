{
  "$files": {
      "array": "extractor_shapeless",
      "fileName": "./{{folder}}/{{output}}_from_{{input}}"
  },
  "$template": {
    "format_version": "1.17.0",
    "minecraft:recipe_shapeless": {
      "description": {
        "identifier": "dw623:{{output}}_from_{{input}}"
      },
      "tags": ["crafting_table"],      
      "priority": 10,
      "ingredients": [
        {"item":"dw623:extractor_coin","count":1},
        {"item":"minecraft:{{input}}","count":"{{=in_count}}"}
      ],
      "result": {
        "item": "minecraft:{{output}}",
        "count": "{{=out_count}}"
      }
    }
  }
}

